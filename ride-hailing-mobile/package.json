{
  "name": "ride-hailing-mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "tunnel": "expo start --tunnel",
    "android": "expo start --android",
    "ios": "expo start --ios", 
    "web": "expo start --web",
    "dev": "expo start --clear",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "setup": "npm install --legacy-peer-deps --ignore-scripts",
    "postinstall": "npx pod-install ios",
    "clean": "expo r -c && npm cache clean --force",
    "clean:all": "npm run clean && cd ios && pod deintegrate && pod install && cd .."
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.2",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "expo": "~53.0.17",
    "expo-constants": "~17.0.3",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~6.0.2",
    "expo-location": "^18.1.6",
    "expo-notifications": "~0.28.2",
    "expo-status-bar": "~2.2.3",
    "jwt-decode": "^4.0.0",
    "pg": "^8.11.3",
    "pg-format": "^1.0.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "^2.27.1",
    "react-native-maps": "^1.24.5",
    "react-native-razorpay": "^2.3.0",
    "react-native-reanimated": "^3.18.0",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.13.1",
    "react-native-svg": "^15.12.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "^0.20.0",
    "wkt": "^0.1.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/react-native": "^12.4.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/react": "~19.0.10",
    "@types/ws": "^8.5.10",
    "babel-preset-expo": "^13.2.3",
    "concurrently": "^8.2.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
